    package="cn.six.aut">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        tools:overrideLibrary="android.support.test.uiautomator.v18">
        <activity
            android:name="cn.six.http.HttpDemo"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="cn.six.espresso.first.OneActivity" />
        <activity android:name="cn.six.espresso.jump.JumpFromActivity" />
        <activity android:name="cn.six.espresso.jump.JumpToActivity" />
        <activity android:name="cn.six.espresso.multiwindow.SuggestActivity" />
        <!-- <activity android:name="cn.six.async.AsyncTaskActivity"/> -->
        <activity android:name="cn.six.espresso.multimatched.MultiMatchedActv" />
        <activity android:name="cn.six.espresso.failhandler.CustomFailHandlerActivity" />

        <activity android:name="cn.six.robolectric.notification.Notification_A"/>
        <activity android:name="cn.six.robolectric.notification.Notification_B"/>
        <activity android:name="cn.six.robolectric.notification.Notification_C"/>
        <activity android:name="cn.six.robolectric.notification.Notification_D"/>
        <activity android:name="cn.six.robolectric.notification.Notification_E"/>

        <service
            android:name="cn.six.service.MyIntentService"
            android:exported="false"></service>
    </application>

</manifest>